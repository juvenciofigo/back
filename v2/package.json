        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "debug": "~2.6.9",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-jwt": "^8.4.1",
        "express-rate-limit": "^7.5.0",
        "express-validation": "^4.1.0",
